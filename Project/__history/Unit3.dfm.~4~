  object ADOQuery2: TADOQuery
    Active = True
    Connection = Form1.ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'SELECT *'
      'FROM Dist_With_Prepod'
      
        'JOIN Form_Of_Control ON Dist_With_Prepod.Form_Of_Control = Form_' +
        'Of_Control.id'
      
        'JOIN Speciality ON Dist_With_Prepod.Speciality_Id = Speciality.i' +
        'd'
      'JOIN Subjects ON Dist_With_Prepod.Name_Dist = Subjects.id')
    Left = 1320
    Top = 80
  end
