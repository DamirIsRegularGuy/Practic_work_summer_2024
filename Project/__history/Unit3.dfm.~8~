  object ADOQuery1: TADOQuery
    Active = True
    Connection = Form1.ADOConnection1
    CursorType = ctStatic
    Parameters = <>
    SQL.Strings = (
      'select * from Prepods'
      'JOIN Kaphedra ON Prepods.Kaphedra = Kaphedra.id'
      'order by First_name')
